{
  "name": "sauls/widget",
  "description": "Simple but powerful widget system for PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Saulius Vaičeliūnas",
      "email": "saulius@vaiceliunas.lt"
    }
  ],
  "require": {
    "php": ">=7.2",
    "sauls/collections": "^1.0",
    "symfony/options-resolver": "^4.0",
    "twig/twig": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Sauls\\Component\\Widget\\": "src/"
    },
    "exclude-from-classmap": [
      "../tests"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sauls\\Component\\Widget\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
