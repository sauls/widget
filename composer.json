{
  "name": "sauls/widget",
  "description": "Simple but powerful widget system for PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Saulius Vaičeliūnas",
      "email": "saulius@vaiceliunas.lt"
    }
  ],
  "require": {
    "php": ">=7.4",
    "sauls/collections": "^1.0",
    "sauls/options-resolver": "^1.0",
    "twig/twig": "^3.0",
    "symfony/cache": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Sauls\\Component\\Widget\\": "src/"
    },
    "exclude-from-classmap": [
      "../tests"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Sauls\\Component\\Widget\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "v1.x-dev"
    }
  }
}
